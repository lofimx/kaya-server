  <rect x="22" y="9" width="1" height="1" fill="#C0C0C0"/>

  <%# Red banner background %>
  <% (19..25).each do |y| %>
    <% (7..22).each do |x| %>
      <rect x="<%= x %>" y="<%= y %>" width="1" height="1" fill="#CC0000"/>
    <% end %>
  <% end %>

  <%# Red banner border - darker red %>
  <% (7..22).each do |x| %>
    <rect x="<%= x %>" y="19" width="1" height="1" fill="#990000"/>
  <% end %>
  <% (19..25).each do |y| %>
    <rect x="7" y="<%= y %>" width="1" height="1" fill="#990000"/>
  <% end %>

  <%# "PDF" text in white pixels %>
