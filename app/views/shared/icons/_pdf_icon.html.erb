<%# Pixelated PDF icon - 32x32 pixel simulation using squares %>
<svg width="64" height="64" viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg" style="image-rendering: pixelated;">

  <%# Document background - white %>
  <% (4..27).each do |y| %>
    <% (6..23).each do |x| %>
      <% next if y < 10 && x > 19 %>
      <rect x="<%= x %>" y="<%= y %>" width="1" height="1" fill="#FFFFFF"/>
    <% end %>
  <% end %>

  <%# Document border - dark gray left and top %>
  <% (4..27).each do |y| %>
    <rect x="6" y="<%= y %>" width="1" height="1" fill="#808080"/>
  <% end %>
  <% (6..19).each do |x| %>
    <rect x="<%= x %>" y="4" width="1" height="1" fill="#808080"/>
  <% end %>

  <%# Document border - black right and bottom %>
  <% (10..27).each do |y| %>
    <rect x="23" y="<%= y %>" width="1" height="1" fill="#000000"/>
  <% end %>
  <% (6..23).each do |x| %>
    <rect x="<%= x %>" y="27" width="1" height="1" fill="#000000"/>
  <% end %>

  <%# Folded corner - diagonal line %>
  <rect x="20" y="4" width="1" height="1" fill="#808080"/>
  <rect x="21" y="5" width="1" height="1" fill="#808080"/>
  <rect x="22" y="6" width="1" height="1" fill="#808080"/>
  <rect x="23" y="7" width="1" height="1" fill="#808080"/>
  <rect x="23" y="8" width="1" height="1" fill="#000000"/>
  <rect x="23" y="9" width="1" height="1" fill="#000000"/>

  <%# Fold area - gray %>
  <rect x="20" y="5" width="1" height="1" fill="#C0C0C0"/>
  <rect x="20" y="6" width="1" height="1" fill="#C0C0C0"/>
  <rect x="20" y="7" width="1" height="1" fill="#C0C0C0"/>
