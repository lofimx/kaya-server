<%# Pixelated IE5-style internet shortcut icon - 32x32 pixel simulation %>
<svg width="64" height="64" viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg" style="image-rendering: pixelated;">
  <style>
    .p { width: 1px; height: 1px; }
  </style>

  <%# Document background - cream/white %>
  <% (4..27).each do |y| %>
    <% (4..21).each do |x| %>
      <% next if y < 10 && x > 17 %>
      <rect x="<%= x %>" y="<%= y %>" class="p" fill="#FFFFFC"/>
    <% end %>
  <% end %>

  <%# Document border - dark gray %>
  <%# Left edge %>
  <% (4..27).each do |y| %>
    <rect x="4" y="<%= y %>" class="p" fill="#808080"/>
  <% end %>
  <%# Bottom edge %>
  <% (4..21).each do |x| %>
    <rect x="<%= x %>" y="27" class="p" fill="#808080"/>
  <% end %>
  <%# Top edge (before fold) %>
  <% (4..17).each do |x| %>
    <rect x="<%= x %>" y="4" class="p" fill="#808080"/>
  <% end %>
  <%# Right edge (below fold) %>
  <% (10..27).each do |y| %>
    <rect x="21" y="<%= y %>" class="p" fill="#808080"/>
  <% end %>

  <%# Folded corner - diagonal %>
  <rect x="18" y="4" class="p" fill="#808080"/>
  <rect x="19" y="5" class="p" fill="#808080"/>
  <rect x="20" y="6" class="p" fill="#808080"/>
  <rect x="21" y="7" class="p" fill="#808080"/>
  <rect x="21" y="8" class="p" fill="#808080"/>
  <rect x="21" y="9" class="p" fill="#808080"/>

  <%# Fold shadow area %>
  <rect x="18" y="5" class="p" fill="#C0C0C0"/>
  <rect x="18" y="6" class="p" fill="#C0C0C0"/>
  <rect x="18" y="7" class="p" fill="#C0C0C0"/>
  <rect x="18" y="8" class="p" fill="#C0C0C0"/>
  <rect x="18" y="9" class="p" fill="#C0C0C0"/>
  <rect x="19" y="6" class="p" fill="#C0C0C0"/>
  <rect x="19" y="7" class="p" fill="#C0C0C0"/>
  <rect x="19" y="8" class="p" fill="#C0C0C0"/>
